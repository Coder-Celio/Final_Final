@model IEnumerable<VehicleList>


@{
    ViewData["Title"] = "Vehicle List";
}

@if(Model.Count() >0)
{
<table class="table">
    <thead>
        <tr>
            <th>Year</th>
            <th>Make</th>
            <th>Model</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var Vehicle in Model)
        {
            <tr>
                <td>@Vehicle.Year</td>
                <td>@Vehicle.Make</td>
                <td>@Vehicle.Model</td>
                <td>
                    @*<a asp-action="@Details" asp-route-id="@Vehicle.Id">Details</a>
                    <a asp-action="@Edit" asp-route-id="@Vehilce.Id" >Edit</a>
                    <a asp-action="@Delete" asp-route-id="@Vehicle.Id">Delete</a>
                    <a href="">Create</a>
                    <a href="">List</a>*@
                </td>
            </tr>
        }

    </tbody>
</table>
}
else
{
    <p class ="text-danger">Sorr No Vehicles Here!</p>
}